{
  "version": "0.0.1",
  "author": {
    "name": "script-money"
  },
  "scripts": {
    "dev": "vite",
    "testnet": "vite --mode testnet",
    "build": "tsc && NODE_ENV=production vite --mode testnet build && yarn cp-libs",
    "cp-libs": "cd browserified-libs && yarn && npx browserify index.js > browserified-libs.js && mv browserified-libs.js ../dist/",
    "serve": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "@onflow/fcl": "^0.0.77",
    "@onflow/types": "^0.0.5",
    "ahooks": "^2.10.11",
    "axios": "^0.22.0",
    "buffer": "^6.0.3",
    "https-browserify": "^1.0.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-date-picker": "^8.3.4",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "stream-http": "^3.2.0",
    "util": "^0.12.4",
    "vite-compatible-readable-stream": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.8",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.3.6",
    "babel-loader": "^8.2.2",
    "browserify-zlib": "^0.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-tailwindcss": "^1.16.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.8",
    "prettier": "^2.4.1",
    "rollup-plugin-polyfill-node": "^0.7.0",
    "storybook-builder-vite": "^0.1.0",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^2.2.16",
    "typescript": "^4.4.3",
    "vite": "^2.6.2"
  },
  "prettier": {
    "eslintIntegration": true,
    "singleQuote": true,
    "semi": false,
    "insertPragma": false,
    "printWidth": 80,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint ./src --fix"
    ]
  }
}
